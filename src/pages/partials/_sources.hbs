<section class="galleryholder projectSection" id="sources">
    <h2 class="standalone">Resources</h2>
    <hr>
    <div class="row">
        <div class="col-md-12">
            <h2>Digital Resources</h2>
            <ul>{{#each index.resources.digital }}
                <li class="col-md-4 gitem"><a href="{{href}}" target="_blank">{{title}}</a></li>{{/each}}</ul>
        </div>
        <div class="col-md-12">
            <h2>Printed Materials</h2>
            <ul>{{#each index.resources.print }}
                <li class="col-md-4 gitem">
                    <h4>{{title}}</h4>
                    <p>{{author}}</p>
                    <p>{{publisher}}</p>
                </li>{{/each}}
            </ul>
        </div>
    </div>
</section>