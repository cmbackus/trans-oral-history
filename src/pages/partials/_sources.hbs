<section class="galleryholder projectSection" id="sources">
    <h2 class="standalone">Resources</h2>
    <hr>
    <div class="row">
        <div class="col-md-6">
            <h2>Digital Resources</h2>
            <ul>{{#each index.resources.print }}
                <li><a href="{{link}}">{{Title}}</a></li>{{/each}}</ul>
        </div>
        <div class="col-md-6">
            <h2>Printed Materials</h2></div>
        <ul>{{#each index.resources.print }}
            <li>
                <h2>{{Title}}</h2>
                <p>{{Author}}</p>
                <p>{{Press}}</p>
            </li>{{/each}}</ul>
    </div>
</section>